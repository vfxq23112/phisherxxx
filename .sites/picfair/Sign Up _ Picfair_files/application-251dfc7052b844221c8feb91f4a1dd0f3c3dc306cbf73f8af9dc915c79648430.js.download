$(document).on('ready', function () {

  if ($('body.support--tickets-controller').length > 0) {

    // --------------------------------------------------------------------------
    // Ticket Subject Auto Suggestions
    // --------------------------------------------------------------------------

    // Debounce Ticket Suggestions to 500ms
    var populateTicketSuggestionsDebounced = _.debounce(function (event) {
      populateTicketSuggestions(event);
    }, 500);

    // Bind onChange event to debounced ticket suggestions
    $('#support_ticket_subject').bind('keyup', populateTicketSuggestionsDebounced);


    // Get ticket suggestions from Zendesk
    function populateTicketSuggestions(event) {
      var url = new URL($('.suggested-articles-body').data('api-url'));
      url.searchParams.set('locale', 'en-us');
      url.searchParams.set('query', $(event.target).val());

      $.ajax({
        type: "GET",
        dataType: "json",
        headers: {
          Accept: "application/json; charset=utf-8"
        },
        url: url,
        success: function (data) {
          setTicketSuggestions(data.results);
        },
        error: function (data) {
          console.error('Error', data);
        }
      });
    };

    // Set the HTML for the ticket suggestions
    function setTicketSuggestions(suggestions) {
      var html = '';

      suggestions.forEach(function (suggestion) {
        html += '<div class="suggestion"><a href="' + suggestion.html_url + '" target="_blank">' + suggestion.name + '</a></div>';
      });

      if (html == '') {
        html = '<div>No suggestions found.</div>';
      }

      $('.suggested-articles').removeClass('hidden');
      $('.suggested-articles-body').html(html);
    }


    // --------------------------------------------------------------------------
    // Ticket Attachment Uploader
    // --------------------------------------------------------------------------

    var ticketUid = $('#support_ticket_uid').val();
    $('#file_upload').fileupload({
      forceIframeTransport: false, // Set to false and add CORS policy to S3 Bucket
      autoUpload: true,
      add: function (event, data) {
        // on file selection

        // clear any previous errors
        clearFormErrors();

        // get a key, policy, and signature from Picfair to submit to Amazon
        $.ajax({
          url: "/support/tickets/" + ticketUid + "/create_attachment",
          type: 'POST',
          dataType: 'json',
          data: {doc: {title: data.files[0].name}},
          async: false,
          success: function (retdata) {
            // put these into our form before it gets submitted to amazon.
            $('#file_upload').find('input[name=key]').val(retdata.key);
            $('#file_upload').find('input[name=policy]').val(retdata.policy);
            $('#file_upload').find('input[name=signature]').val(retdata.signature);
          }
        });

        // send the file directly sent to Amazon using the form data
        data.submit()
      },
      send: function () {
        // start loader & disable form
        isLoading(true);
      },
      fail: function (e, data) {
        // stop loading
        isLoading(false)

        console.error('Upload Failure:', data.jqXHR);
        supportFormError('Uploading failed. Please try a different file.')
      },
      done: function (event, data) {
        var file = data.files[0];
        var attachmentsDiv = $('#new_support_ticket .attachments');
        var s3Key = $('#file_upload').find('input[name=key]').val();

        var fileInfo = {filename: file.name, type: file.type, size: file.size, s3_key: s3Key};
        attachmentsDiv.append('<li>' + file.name + ' (' + file.size + ' bytes)' +
          '<input type="hidden" name="support_ticket[files_json][]" value="' + _.escape(JSON.stringify(fileInfo)) + '"></li>');

        // stop the loader
        isLoading(false);
      },
    })

    // loading
    isLoading = function (loading) {
      if (loading) {
        $('.upload-spinner').show();
        $('.support-form-submit').attr('disabled', true);
      } else {
        $('.upload-spinner').hide();
        $('.support-form-submit').removeAttr('disabled');
      }
    }

    // upload errors
    var fileUploadErrors = $('.file-upload-errors');

    clearFormErrors = function () {
      // clear form errors
      fileUploadErrors.hide();
      fileUploadErrors.text('');
    }

    supportFormError = function (errorMsg) {
      clearFormErrors();
      fileUploadErrors.text(errorMsg);
      fileUploadErrors.show();
    }
  }
});

