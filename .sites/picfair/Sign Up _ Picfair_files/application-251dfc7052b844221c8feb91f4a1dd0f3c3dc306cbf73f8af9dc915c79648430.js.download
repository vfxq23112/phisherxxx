$(document).bind("page:load ready", function(){
  var $window = $(window);
  var $items, $container;

  if ($('.slideshow-trigger, .zoom-pic').length) {
    if ($('.slideshow-old').length) {
      // Old Slideshow
      $(window).on('resize', function () {
        resize();
      });
    }

    $(document).ajaxComplete( function() {
      if (typeof(glide) !== 'undefined') {
        glide.destroy();
        glide = undefined;
      }
    });

    // This same handler is used for old and new slideshows
    $('body').on('click', '.slideshow-trigger, .zoom-pic', function(e){
      e.preventDefault();

      var start = 0;
      if ($(this).hasClass('zoom-pic')) {
        start = $('.zoom-pic').index($(this));
      }

      $('.pic_list_slideshow').show();
      $('body').css("overflow", "hidden");

      if ($('.slideshow-new').length) {
        // New Slideshow

        if (typeof(glide) === 'undefined') {
          glide = new Glide('.glide', {
            type: 'carousel',
            startAt: start,
            perView: 1,
            animationDuration: 300,
            animationTimingFunc: 'linear',
            dragThreshold: false
          }).mount();
          checkWindowSizeChange();
        }
        else {
          // Updating the existing glide instance is MUCH faster than making a new one every time
          glide.settings.animationDuration = 0;
          glide.go("="+start);
          glide.settings.animationDuration = 300;

          if (checkWindowSizeChange()) {
            glide.update();
          }
        }

      } else {
        // Old Slideshow
        $('.pic_list_slideshow .slideshow-pic img').each(function () {
          var pic_url = $(this).data('img-src');
          $(this).attr('src', pic_url);
        });

        setUpPicListSlideshow(resize, start);
      }

      // Esc can close both slideshows
      $(document).on("keyup.escKeyClosesModal", function(e) {
        if (e.keyCode == 27) {
          closeSlideShow();
        }
      });

    });

    $('body').on('click', '.slideshow-wrapper .slideshow-prev', function() {
      $(this).closest('.slideshow-wrapper').microfiche({slideByPages: -1});
    });

    $('body').on('click', '.slideshow-wrapper .slideshow-next', function() {
      $(this).closest('.slideshow-wrapper').microfiche({slideByPages: 1});
    });

    $('body').on('click', '.slideshow-pic-holder-box', function(e) {
      link = $(this).find('img').data('url');

      // If cmd / ctrl is held down...
      if (e.ctrlKey || e.metaKey) {
        window.open(link, '_blank');
      }
    });

    var getSlides = $('.slideshow-slide').length;

    $('.slide-number').text(getSlides);

    $('.slideshow-slide').each(function(i){
      $(this).attr("data-number", (i+1));
      $(this).find('.slide-count').text(i+1 + ' / ');
    });

    var boardTitle = $('.slideshow-wrapper').data('name');

    if (boardTitle) {
      var boardAmount = $('.slideshow-wrapper').data('amount');
      $('.slideshow-wrapper').append('<div class="slideshow-slide"><div class="slide-panels"><div class="slideshow-pic"><div class="slideshow-end"><div class="slideshow-end-title">' + boardTitle +'</div><div class="slideshow-end-amount">' +boardAmount + ' images</div></div></div></div></div>')
    }

    $('body').on('click', '.slide-exit', function() {
      closeSlideShow();
    });
  }

  function checkWindowSizeChange() {
    var new_ws = getSlideWindowSize();
    // console.log("Checking window size", window.slideWindowSize, new_ws);
    var changed = (new_ws !== window.slideWindowSize);
    window.slideWindowSize = new_ws;

    return changed;
  }

  function getSlideWindowSize() {
    var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
    var h = Math.max(document.documentElement.clientHeight, window.innerHeight || 0);

    return ""+w+"x"+h;
  }

  function closeSlideShow() {
    document.body.style.overflow = 'visible';
    if ($('.slideshow-old').length) {
      $('.slide-title, .slide-price, .pic_list_slideshow img').animate({'opacity': '0'});
      $('.basket_breakdown_totals_sidebar_holder').css('bottom', '52px');
    }
    $('.pic_list_slideshow').hide();
    $(document).off("keyup.escKeyClosesModal");
  }

  function imageActionType(e) {
    var image = $(e.target)['0'];
    var horizontalClickPercent = e.offsetX / image.clientWidth * 100;
    if (horizontalClickPercent < 20)
      return 'prev';
    else if (horizontalClickPercent > 80)
      return 'next';
    else {
      return 'go';
    }
  }

  function setUpPicListSlideshow(callback, start) {
    $('.pic_list_slideshow').each(function() {
      var $carousel = $(this);
      $container = $carousel.find('.slideshow-wrapper');
      $items = $carousel.find('.slideshow-slide');

      var cyclic = start == 0 ? true : false;

      $container.microfiche({
        cyclic: cyclic,
        refreshOnResize: false, // Turning this on causes problems on Open Slideshow, Close Slideshow, Resize Browser, Open Slideshow
        bullets: false,
        keyboard: true,
        buttons: true,
        jumpToPage: start
      }, resize());
    });

    callback();
  }

  function resize() {
    // $('.slide-title, .slide-price, .pic_list_slideshow img').animate({'opacity': '0'});

    if ($container && $container.data('microfiche')) {
      var m = $container.data('microfiche');
      var translate = -(m.currentPageIndex() * $('.slideshow-slide').first().outerWidth());
      $('.microfiche-film').css('transform', 'translate3d(' + translate + 'px, 0px, 0px)');
    }

    setTimeout(function(){
      $('.slideshow-slide').each(function(i){
        var imgW = $(this).find('img').width();
        var imgWavebreak = $(this).find('img.slideshow-wavebreak').width();
        var doubleSize = imgWavebreak  * 2;
        $(this).children().children('.slide-title, .slide-price').css({ 'width': imgW + 'px'})
        $(this).children().children('.wavebreak-slideshow-title, .wavebreak-slideshow-price').css({ 'width': doubleSize + 'px'});
      });
      $('.slide-title, .slide-price, .pic_list_slideshow img').animate({'opacity': '1'});
    },1000);

    if ($items) {
      $items.css({ width: Math.floor($container.width()) });
    }
  }
});
