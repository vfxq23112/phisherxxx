$(function() {
  $('.image-panel [src]').each(showImage);

  $(window).on('scroll load', function() {
    $('.image-panel [data-src]').each(function() {
      var $img = $(this);

      if (isOnScreen($img, 200)) {
        $img.attr('src', $img.data('src'));
        $img.removeAttr('data-src');
        $img.each(showImage);
      }
    });
  });

  function isOnScreen(el, padding) {
    padding = padding || 0;

    var scrollTop = $(window).scrollTop();
    var scrollBottom = scrollTop + $(window).height();
    var top = $(el).offset().top;
    var bottom = top + $(el).outerHeight();

    return top - padding < scrollBottom && bottom + padding > scrollTop;
  }

  function showImage() {
    var $image = $(this);

    $image.imagesLoaded(function() {
      $image.fill({
        width: $image.width(),
        height: $image.height()
      });

      $image.addClass('is-visible');
    });
  }

  $(window).on('scroll load', function() {
    $('.slide-panels [data-src]').each(function() {
      var $img = $(this);
      if (isOnScreen($img, 200)) {
        $img.attr('src', $img.data('src'));
        $img.removeAttr('data-src');
        $('.slideshow-slide').each(function(i){
          var imgW = $(this).find('img').width();
            $(this).children('.slide-price, .slide-title').css({ 'width': imgW + 'px'})
        });
      }
    });
  });

});
window.searchMeta = function() {
  var $window = $(window);

  $('.search-meta').each(function() {
    var $filterBar = $(this);

    var $appliedFilters = $filterBar.find('.search-meta__applied-filters');
    var $appliedFiltersContent = $filterBar.find('.search-meta__applied-filters__content');
    var $topBar = $filterBar.find('.search-meta__top-bar');
    var $topBarCols = $filterBar.find('.search-meta__top-bar__column');

    positionAppliedFilters();
    $window.on('resize', positionAppliedFilters);

    function positionAppliedFilters() {
      var appliedFiltersContentWidth = $appliedFiltersContent.width();
      var appliedFiltersTopMargin = '';
      var filterBarWidth = $filterBar.width();
      var topBarHeight = $topBar.height();

      var topBarColOneWidth = $topBarCols.eq(0).width();
      var topBarColTwoWidth = $topBarCols.eq(1).width();
      var availableSpace = filterBarWidth / 2 - appliedFiltersContentWidth / 2;

      if (availableSpace > topBarColOneWidth && availableSpace > topBarColTwoWidth) {
        appliedFiltersTopMargin = -topBarHeight;
        $appliedFilters.removeClass('search-meta__applied-filters--wrapped')
      } else {
        $appliedFilters.addClass('search-meta__applied-filters--wrapped')
      }

      $appliedFilters.css({
        marginTop: appliedFiltersTopMargin
      });
    }
  });
};

