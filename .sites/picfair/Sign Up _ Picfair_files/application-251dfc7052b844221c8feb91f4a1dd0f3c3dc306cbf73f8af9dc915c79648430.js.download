      if (width_value < 350) { width_value = 350; }

      $('input[type=range]').val(width_value);
      $(this).val(width_value + 'px');
      changeImageEmbedCode(embed_code, width_value, title, branding);
    });

    $('input#hide_title').change(function() {
      if ($(this).is(":checked")) {
        title = "false";
      } else {
        title = "true";
      }

      changeImageEmbedCode(embed_code, width_value, title, branding);
    });

    $('input#hide_branding').change(function() {
      if ($(this).is(":checked")) {
        branding = "false";
      } else {
        branding = "true";
      }

      changeImageEmbedCode(embed_code, width_value, title, branding);
