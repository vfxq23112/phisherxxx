function changeImageEmbedCode(code, width, title, branding) {
  code = code.replace(/data-width="\w+"/, 'data-width="' + width + '"');
  code = code.replace(/data-title="\w+"/, 'data-title="' + title + '"');
  code = code.replace(/data-branding="\w+"/, 'data-branding="' + branding + '"');

  $('textarea').val(code);
  $('.embed-container.image').html(code);
};
