LIST_MODAL_TOOLS = {
  closeModal: function() {
    $('.list_modal_holder').fadeOut(200, function() {
      $('.multiselected').removeClass('multiselected');
      $('.pic_selection_overlay').hide();

      // Reset modal
      $('.list_modal.list_success_modal').fadeOut(200, function() {
        $('.list_modal__button').removeClass('active');
        $('.list_modal.list_multi_modal').show();
      });
    });
  },

  openModal: function(modal) {
    if ($('.multiselected').length) {
      $('.multiselect__image-count').text($('.multiselected').length + " images");
    } else {
      $('.multiselect__image-count').text("1 image");
    }

    $(modal).fadeIn(400);
  },

  showSuccess: function() {
    $('.list_modal.list_multi_modal').fadeOut(400, function() {
      $('.list_modal.list_success_modal').fadeIn(400);
    });

    $('.multi_save_close_button').click(function(e) {
      e.preventDefault();
      LIST_MODAL_TOOLS.closeModal();
    });
  },

  createList: function(list_type, pics, name, lists) {
    $.post('/pic_' + list_type + 's/create_with_multiple/' + pics + '?name=' + encodeURIComponent(name), function(e) {
      if (!lists.length) {
        if (e) {
          if (e.succeeded_count > 1) {
            var main_message = e.succeeded_count + " pics have been added to " + name;
          } else {
            var main_message = "1 pic has been added to " + name;
          }

          if (e.not_for_board_count > 1) {
            var not_added_message = e.not_for_board_count + " pics have not been added to " + name + " as you cannot add your own pics to a board.";
            $('.list_success_modal p').text(not_added_message);
          } else if (e.not_for_board_count == 1) {
            var not_added_message = "1 pic has not been added to " + name + " as you cannot add your own pics to a board.";
            $('.list_success_modal p').text(not_added_message);
          }
        } else {
          var main_message = pics.length + " pics have been added to " + name;
        }

        $('input#new_list_name').val('');
        $('.list_success_modal h2').text(main_message);
        LIST_MODAL_TOOLS.showSuccess();

        // Refresh list of boards
        if (list_type == 'board' || list_type == 'album') {
          $('.list_button_holder').empty();

          $.each(e.user_lists, function(i, two_lists) {
            var html = '<div class="list_row">';
            $.each(two_lists, function(i, list) {
              html += '<a href="#" data-list-id="' + list.id + '" class="list_modal__button ' + list.css_class + '">' + list.name + '</a>';
            });
            html += '</div>'

            $('.list_button_holder').append(html);
          });
        }
      }
    }).fail(function() {
      var error_message = "Sorry, there was an error creating your " + list_type + ". Please try again.";
      $('.list_error_holder').text(error_message);
    });
  }
};
$(function() {
  var MULTISELECT_PAGE_CHANGE_CONFIRMATION = 'You have selected some images. If you leave this page, your changes will not be saved. Are you sure you want to continue?';

  $('body').on('click', '.select-pic', function(e) {
    e.preventDefault();
    $(this).closest('.pic').find('.pic_selection_overlay').toggle().toggleClass('multiselected');
    dealWithPicSelection();
  });

  $('body').on('reset', '.multiselect__select-all', function(e) {
    e.preventDefault();

    $('.pic_selection_overlay').removeClass('multiselected').hide();

    dealWithPicSelection();
  });

  $('body').on('click', '.multiselect__select-all', function(e) {
    e.preventDefault();

    if ($(this).hasClass('deselect-all')) {
      $('.pic_selection_overlay').removeClass('multiselected').hide();
    } else {
      $('.pic_selection_overlay').show().addClass('multiselected');
    }

    dealWithPicSelection();
  });

  if ($('.multiselect').length) {
    window.addEventListener("beforeunload", function (e) {
      if ($('.multiselected').length) {
        e.returnValue = MULTISELECT_PAGE_CHANGE_CONFIRMATION;
        return MULTISELECT_PAGE_CHANGE_CONFIRMATION;
      }
    });
  }

  function dealWithPicSelection() {
    var selected_count = $('.multiselected').length;

    if ($('.multiselect__add-to-album').length) {
      var type = "album";
      var add_button = $('.multiselect__add-to-album a');
    } else {
      var type = "board";
      var add_button = $('.multiselect__add-to-board a');
    }

    var plurality = selected_count > 1 ? " images " : " image ";
    var prev_plurality = selected_count > 1 ? " previews" : " preview";
    $('.actions__view_in_slideshow').text('View ' + selected_count + plurality + 'in slideshow');
    $('.actions__add-to-board').not('.zoom-mode-button').text('Add ' + selected_count + plurality + 'to board');
    $('.actions__download-multiple-previews').text('Download ' + selected_count + prev_plurality);
    $('.actions__remove-from-board').text('Remove ' + selected_count + plurality + 'from board');
    $('.actions__add-to-album').not('.zoom-mode-button').text('Add ' + selected_count + plurality + 'to album');
    // $('.actions__add-to-basket').text('Add ' + selected_count + plurality + 'to basket');
    $('.actions__logged-out.board-add a').text('Add ' + selected_count + plurality + 'to board');
    $('.actions__add-to-board.add-board-show').text('Add ' + selected_count + plurality + 'to another board');


    if (selected_count) {
      $('.actions_table ul li.selected-images-action').removeClass('disabled');
      add_button.text("Add " + selected_count + " images to " + type);
      add_button.removeClass('disabled');

      $('.multiselect__select-all').text('Deselect All');
      $('.multiselect__select-all').addClass('deselect-all');
    } else {
      $('.actions_table ul li.selected-images-action').addClass('disabled');
      add_button.text("Add images to " + type);
      add_button.addClass('disabled');

      $('.multiselect__select-all').removeClass('deselect-all');
      $('.multiselect__select-all').text('Select All');
    }
  }
});
$(function() {
  if ($('.pic_list_sortable .pic').length) {
    var columns = 4;
    var column_margin = '2.35765%';
    var arrow_offset = $('.pic__content').width() - 80;

    $('.pic_list_sortable').sortable({
      containment: "parent",
      cursor: "move",
      cursorAt: { left: 18, top: arrow_offset },
      helper: "original",
      items: ".pic",
      opacity: 0.7,
      revert: 100,
      handle: '.reorder_image',
      sort: function() { setPicGrid(); },
      start: function() { setPicGrid(); },
      stop: function() { setPicGrid(); },
      tolerance: "pointer",
      update: function() {
        var list_type = $('.pic_list_sortable').data('list-type');
        var ids_in_new_order = $('.pic').map(function() { return $(this).data('pic-id'); }).get();
        var list_id = $('.pic_list_show').data('list-id');
        $.post('/pic_' + list_type + 's/' + list_id + '/change_order/' + ids_in_new_order).done(function(e) {
          var html = e.slideshow_html;
          $('.slideshow__holder').html(html);
        });
      }
    });

    function setColumns() {
      var width = $(window).width();

      if (width > 2060) {
        columns = 6; column_margin = "2.35765%";
      } else if (width > 1024) {
        columns = 4; column_margin = "2.35765%";
      } else if (width >= 627) {
        columns = 2; column_margin = "2.35765%";
      } else {
        columns = 1; column_margin = "0";
      }
    }

    function setPicGrid() {
      setColumns();

      $('.pic').css('margin-right', column_margin);
      $('.pic').css('clear', 'none');

      if ($('.category__layout__sidebar').length && $(window).width() >= 1040) {
        var description_row_span = Math.ceil($('.category__layout__sidebar').height() / $('.pic').height());
        var short_row_indexes = [];
        var clear_left_indexes = [];

        for (i=1; i <= description_row_span; i++) {
          clear_left_indexes.push(i * (columns - 1));
          short_row_indexes.push((i * (columns - 1)) - 1);
        }

        $('.pic').not('.ui-sortable-helper').filter(function(i) { return short_row_indexes.indexOf(i) != -1; }).css('margin-right', 0);
        $('.pic').not('.ui-sortable-helper').filter(function(i) { return clear_left_indexes.indexOf(i) != -1; }).css('clear', 'left');

        var rest_of_pics = $('.pic').not('.ui-sortable-helper').filter(function(i) { return i > short_row_indexes[short_row_indexes.length-1] });
        rest_of_pics.filter(function(i) { return (i + 1) % columns == 0 }).css('margin-right', 0);
        rest_of_pics.filter(function(i) { return i % columns == 0 }).css('clear', 'left');
      } else {
        $('.pic').not('.ui-sortable-helper').filter(function(i) { return (i + 1) % columns == 0 }).css('margin-right', 0);
        $('.pic').not('.ui-sortable-helper').filter(function(i) { return i % columns == 0 }).css('clear', 'left');
      }
    }

    $(window).resize(function() { setPicGrid(); }).resize();
  };

  if ($('.album_list_sortable .album').length > 1) {

    $('.album_list_sortable').sortable({
      axis: "y",
      containment: ".album_list",
      cursor: "move",
      update: function() {
        var user = $('table.list_table').data('user');
        var album_ids_in_new_order = $('.album').map(function() { return $(this).data('album-id'); }).get();
        $.post('/users/' + user + '/change_album_order/' + album_ids_in_new_order).done(function(e) {

        });
      }
    });

  }

});
