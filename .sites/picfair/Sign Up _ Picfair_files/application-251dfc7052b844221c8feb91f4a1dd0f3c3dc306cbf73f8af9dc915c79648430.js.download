function getPreviewImages(callback) {
  $('#preview-images').html("");
  var size = $('#size option:selected').data('size-inches')
  var order_id = $('#print_order_id').val()

  // create preview URL
  var cropData = JSON.parse($('#crop-data')[0].value);
  var rawImageWidth = $('.pic-preview').data('raw-image-width');
  var rawImageHeight = $('.pic-preview').data('raw-image-height');
  var xScale = rawImageWidth / cropData.originalWidth;
  var yScale = rawImageHeight / cropData.originalHeight;

  var previewUrl = new URL(window.location.origin + '/print/' + order_id + '/step4_show_preview_images');
  previewUrl.searchParams.append('size', size);
  previewUrl.searchParams.append('crop[x]', Math.floor(cropData.x * xScale));
  previewUrl.searchParams.append('crop[y]', Math.floor(cropData.y * yScale));
  previewUrl.searchParams.append('crop[width]', Math.floor(cropData.width * xScale));
  previewUrl.searchParams.append('crop[height]', Math.floor(cropData.height * yScale));

  $.ajax({
    type: "GET",
    dataType: "html",
    headers: {
      Accept: "application/json; charset=utf-8"
    },
    url: previewUrl,
