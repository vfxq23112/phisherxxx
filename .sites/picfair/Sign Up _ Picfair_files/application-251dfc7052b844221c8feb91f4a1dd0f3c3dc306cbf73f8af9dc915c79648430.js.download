      tagValue = '"'+tagValue+'"';

    inputField.val(inputField.val() + " " + tagValue);

    $('.searchpanel #new_search').submit();
  });

  $('body').on('mouseover', '.not-tag', function (event) {
    var termItem = $($(this).closest('.associated-terms.item'));
    termItem.addClass('fake-not-tag');
  });
  $('body').on('mouseout', '.not-tag', function (event) {
    var termItem = $($(this).closest('.associated-terms.item'));
    termItem.removeClass('fake-not-tag');
  });

  $('body').on('click', '.not-tag', function (event) {
    event.preventDefault();
    event.stopPropagation();
    var tagValue = $(this).data("term");

    var inputField = $('#secondary-search');

    if (tagValue.indexOf(' ') > -1)
      tagValue = '"'+tagValue+'"';

    inputField.val(inputField.val() + " " + "-" + tagValue);

    $('.searchpanel #new_search').submit();
  });

  $('body').on('click', '.remove-tag', function (event) {
    event.preventDefault();
    event.stopPropagation();
    var tagValue = $(this).data("term");

    if ($('#secondary-search').length > 0) {
      var currentSearchValue = $('#secondary-search').val();
    }

    // Removes current term and extra whitespace
    var newSearchValue = updateQueryString(currentSearchValue, tagValue);
    newSearchValue = newSearchValue.trim();

    var inputField = $('#secondary-search');
    inputField.val(newSearchValue);

    $('.searchpanel #new_search').submit();
  });

  function updateQueryString(query, remove_tag) {
    var terms = query.toLowerCase().match(/"[^"]*"|\S+/g);
    var keep_terms = [];

    for(var i=0; i<terms.length; i++) {
      var term = terms[i];
      if (term != remove_tag && term != '"'+remove_tag+'"')
        keep_terms.push(term);
    }

    return keep_terms.join(' ');
  };

  $('.search-terms').slick({
    variableWidth: true,
    infinite: false
  });

  window.countVisibleSlides = function(){
    var margin = 10;
    var sliderWidth = $('.slick-list').width();
    var accumulatedWidth = 0;
    var slides = $('.slick-slide');
    var index = 0;

    for (; accumulatedWidth < sliderWidth; index++) {
      var slide = $(slides[index]);
      accumulatedWidth += slide.width() + margin;
    }

    if (index <= 1){
      return 1;
    }else {
      return index - 1;
    }
  };

  function setSideScrollSize(){
    $('.search-terms.slick-slider').slick('slickSetOption', 'slidesToScroll', window.countVisibleSlides(), true);
  }
  setSideScrollSize();
  $( window ).resize(function() {
