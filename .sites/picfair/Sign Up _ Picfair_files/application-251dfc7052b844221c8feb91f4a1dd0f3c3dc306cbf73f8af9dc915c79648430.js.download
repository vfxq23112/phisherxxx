// Loading Animation
function nextStepLoaderAnimation(action, currentButton, loaderColor) {
  var contentContainer = $('#print-page-container');
  var loadAnimation = $('.print-spinner').first().clone();

  if (loaderColor == 'black') {
    loadAnimation.find('.spinner').addClass('print-spinner-black');
  }

  if (currentButton.find('.print-spinner').length < 1) {
    if (action === 'start') {
      $(contentContainer).addClass('loading-overlay');
      currentButton.find('span').hide();
      currentButton.append(loadAnimation);
      loadAnimation.show();

    } else if (action === 'stop') {
      $(contentContainer).removeClass('loading-overlay');
      currentButton.find('span').show();
      currentButton.find('.print-spinner').remove();
    }
  }
}
