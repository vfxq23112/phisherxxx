$(document).bind("page:load ready", function() {
  // jQuery plugin to prevent double submission of forms
  // Use it like this:
  //
  // $('form').preventDoubleSubmission();
  // If there are AJAX forms that should be allowed to submit multiple times per page load, you can give them a class indicating that, then exclude them from your selector like this:
  //
  // $('form:not(.js-allow-double-submission)').preventDoubleSubmission();
  jQuery.fn.preventDoubleSubmission = function() {
    $(this).on('submit',function(e){
      var $form = $(this);

      if ($form.data('submitted') === true) {
        // Previously submitted - don't submit again
        e.preventDefault();
      } else {
        // Mark it so that the next submit can be ignored
        $form.data('submitted', true);
      }
    });

    // Keep chainability
    return this;
  };

  $('form.prevent-double-submission').preventDoubleSubmission();
});
$(function () {

  $('.mobile-review-details .expand-arrow').on('click', function () {
    if($('.collapsed-section').is(":visible")) {
      $('.mobile-review-details .collapsed-section').hide();
      $('.mobile-review-details .expand-arrow .fa-caret-down').show();
      $('.mobile-review-details .expand-arrow .fa-caret-up').hide();
    } else {
      $('.mobile-review-details .collapsed-section').show();
      $('.mobile-review-details .expand-arrow .fa-caret-down').hide();
      $('.mobile-review-details .expand-arrow .fa-caret-up').show();
    }
  });

  $('.card_basket_breakdown_holder').on('click', '.cart-expand', function(e){
    e.preventDefault();
    $('.card_basket_breakdown_holder .details-page').show();
    $('.card_basket_breakdown_holder .summary-page').hide();
  });

  $('.card_basket_breakdown_holder').on('click', '.cart-contract', function(e){
    e.preventDefault();
    $('.card_basket_breakdown_holder .details-page').hide();
    $('.card_basket_breakdown_holder .summary-page').show();
  });

  $('#print-checkout-main-content').on('click', '.new-card', function(e){
    e.preventDefault();
    $('#new-card-container').removeClass('hidden');
    $('#existing-card-container').hide();
  });

  if ($('#new_order').length > 0) {
    $('#new_order').on('submit', function (e) {
      e.preventDefault();
    });

    $('#new_order').parsley().on('form:submit', function(e) {
      processStripe();
    });
  }

  function processStripe(){
    Stripe.setPublishableKey($('meta[name=stripe-key]').attr('content'));

    Stripe.createToken({
      number: $('#order_card_number').val(),
      cvc: $('#order_card_cvc').val(),
      exp_month: $('#order_card_date').payment('cardExpiryVal').month,
      exp_year: $('#order_card_date').payment('cardExpiryVal').year,
      address_zip: $('#order_card_postcode').val()
    }, handleStripeResponse);
  }

  handleStripeResponse = function(status, response) {
    if (status === 200) {
      var hiddenForm = $('#new_hidden_order');
      hiddenForm.find('input#stripe_card_token').val(response.id);
      hiddenForm.find('input#billing_address_email').val($('#order_email').val());
      hiddenForm.find('input#save_card_details').val($('#save_card_details:checked').length > 0);

      $('#new_hidden_order').submit();

    } else {
      displayErrorModal(response.error.message);
      return false;
    }
  };

  function displayErrorModal(response) {
    console.log("Hitting display error modal");
    var details = "(" + response + ")";

    $('.modal').addClass('open');
    $('.modal .spinner').hide();
    $('.modal-header h3').addClass("error");
    $('.modal-header h3').text("We've had a problem processing your order! " + details);
    $('.modal-content p').html('Please check your details and try again or <strong><a href="https://www.picfair.com/support/tickets/new">contact us</a></strong> for help.');
    $('.modal-actions').html("<a href='#' class='button dismiss-modal'>Ok</a>");

    return $('input[type=submit]').removeAttr('disabled');
  }

  $('#print-checkout-main-content').on('click', '.dismiss-modal', function(event) {
    event.preventDefault();
    $('.modal').removeClass('open');
  });

  // Custom Parsley Validations
  window.Parsley.addValidator('cardNumber', {
    validateString: function(value) {
      return $.payment.validateCardNumber(value);
    },
    messages: {
      en: 'The card number must be valid'
    }
  });

  window.Parsley.addValidator('cardExpiry', {
    validateString: function(value) {
      var date = $.payment.cardExpiryVal(value);
      var month = date.month;
      var year  = date.year;

      return jQuery.payment.validateCardExpiry(month, year);
    },
    messages: {
      en: 'Card expiry must be valid'
    }
  });

  window.Parsley.addValidator('cardCvc', {
    validateString: function(value) {
      return $.payment.validateCardCVC(value);
    },
    messages: {
      en: 'Card CVC must be valid'
    }
  });

  // Format Inputs
  function formatInputs() {
    $('input#order_card_number').payment('formatCardNumber');
    $('input#order_card_date').payment('formatCardExpiry');
    $('input#order_card_cvc').payment('formatCardCVC');
  }

  // Set error class for Parsley
  return $('form#new_order').each(function() {
    formatInputs();
    return $(this).parsley({
      errorClass: 'error'
    });
  });

});
