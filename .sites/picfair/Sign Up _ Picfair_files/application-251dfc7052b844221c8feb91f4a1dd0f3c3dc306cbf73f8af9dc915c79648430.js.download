$(function() {
  var board_name_interval;

  $('.rename-list-button').click(function(e) {
    e.preventDefault();
    $('.name-edit-error').remove();
    var $name = $('.pic_list__name h1');
    $name.attr('contenteditable', 'true').focus();
    setEndOfContenteditable(document.getElementById('list_name'));

    $name.keypress(function(e){ return e.which != 13; });

    // Save new list name every three seconds if it has changed
    board_name_interval = setInterval(function() { saveNewTitle();}, 3000);
  });

  if ($('#list_name').length) {
    var list_title = $('#list_name').html();
    var new_title = "";
    var list_id = $('.pic_list_show').data('list-id');
  }

  function saveNewTitle() {
    new_title = $('#list_name').html();
    if (new_title != list_title) {
      $.post('/pic_boards/' + list_id + '/change_list_name/' + encodeURIComponent(new_title), function() {
        $('.actions_table ul li:first-of-type').addClass('button-saved').text('Saved!');
        setTimeout(function(){
          $('.actions_table ul li:first-of-type').removeClass('button-saved').text('Options');
        },2000);

        list_title = new_title;
      }).fail(function(e) {
        $('.pic_list_show__header').after("<div class='name-edit-error'>Sorry, you cannot edit the title of this board.</div>");
        clearInterval(board_name_interval);
      });
    }
  }

  // function to focus the name field at the end of the content
  function setEndOfContenteditable(contentEditableElement) {
    var range,selection;
    if (document.createRange) {
      range = document.createRange();
      range.selectNodeContents(contentEditableElement);
      range.collapse(false);
      selection = window.getSelection();
      selection.removeAllRanges();
      selection.addRange(range);
    } else if (document.selection) {
      range = document.body.createTextRange();
      range.moveToElementText(contentEditableElement);
      range.collapse(false);
      range.select();
    }
  }
});
