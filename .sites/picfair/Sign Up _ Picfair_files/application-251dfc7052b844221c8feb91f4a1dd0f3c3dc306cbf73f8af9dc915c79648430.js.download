$(document).on('ready', function () {

  if ($('body.account-controller.prints-action, body.account-controller.prints-update-action').length > 0) {

    $("form#opt-in-my-images-to-prints input[type=text]").focus(function() {
      var save_this = $(this);
      window.setTimeout (function(){
        save_this.select();
      },100);
    });

    function recalculatePrintMargins() {
      var VAT_RATE = 0.2;
      var pf_commission_percentage = $('#account_type').find(':selected').data("picfairCommissionPct");
      var photographerCommissionMultiplier = Number($('#account_type').find(':selected').data("photographer-commission-multiplier"));
      var PF_COMMISSION_RATE = pf_commission_percentage / 100;
      var photographerPrintCommisionPct = Number($('#user_extra_print_product_commission_pct').val()) * photographerCommissionMultiplier;
      var vatRegistered = ($('#users_is_vat_registered').val() == 'true');
      var sampleLicencePrice = Number($('#sample_licence_price').val());
      var exampleProductPriceIncVat = Number($('#example_product').val());

      // Calculate licence price inc/ex VAT
      if (vatRegistered) {
        var sampleLicencePriceExVat = sampleLicencePrice / (1.0 + 1.0 * VAT_RATE);
        var sampleLicencePriceIncVat = sampleLicencePrice;
      }
      else {
        var sampleLicencePriceExVat = sampleLicencePrice;
        var sampleLicencePriceIncVat = sampleLicencePrice + sampleLicencePrice * VAT_RATE;
      }

      // Calculate Picfair Commission
      var picfairCommissionExVat = sampleLicencePriceExVat * PF_COMMISSION_RATE;
      var picfairCommissionIncVat = picfairCommissionExVat + picfairCommissionExVat * VAT_RATE;

      // Calculate Example Product Price Ex VAT
      var exampleProductPriceExVat = exampleProductPriceIncVat / (1.0 + 1.0 * VAT_RATE);

      // Calc Photographer Print Commission
      var photographerPrintCommissionExVat = photographerPrintCommisionPct / 100.0 * exampleProductPriceExVat;
      var photographerPrintCommissionIncVat = photographerPrintCommissionExVat + photographerPrintCommissionExVat * VAT_RATE;
      var photographerPrintCommission = vatRegistered ? photographerPrintCommissionIncVat : photographerPrintCommissionExVat;

      // Calc Customer will pay
      var customerWillPayExVat = sampleLicencePriceExVat + picfairCommissionExVat + exampleProductPriceExVat + photographerPrintCommissionExVat;
      var customerWillPayIncVat = customerWillPayExVat + customerWillPayExVat * VAT_RATE;

      // Calc combined product & picfair cost
      var productAndPicfairCostExVat = picfairCommissionExVat + exampleProductPriceExVat;
      var productAndPicfairCostIncVat = productAndPicfairCostExVat + productAndPicfairCostExVat * VAT_RATE;

      // Calc you will receive
      var youWillReceiveValueExVat = photographerPrintCommissionExVat + sampleLicencePriceExVat;
      var youWillReceiveValueIncVat = youWillReceiveValueExVat + youWillReceiveValueExVat * VAT_RATE;
      var youWillReceiveValue = vatRegistered ? youWillReceiveValueIncVat : youWillReceiveValueExVat;

      $('#photographer-print-commission-value').html(photographerPrintCommission.toFixed(2));
      $('#grand-total-value').html(customerWillPayIncVat.toFixed(2));
      $('#you-will-receive-value').html(youWillReceiveValue.toFixed(2));
      $('#photographer-print-commission-pct').html(photographerPrintCommisionPct);
      $('#picfair-print-commission-pct').html(pf_commission_percentage);
      $('#photographer-print-commission-value').html(photographerPrintCommission.toFixed(2));
      $('#picfair-will-receive-value').html(productAndPicfairCostIncVat.toFixed(2));
      $('#picfair-commission-value').html(picfairCommissionIncVat.toFixed(2));

      checkForNan = [$('#photographer-print-commission-value'), $('#grand-total-value'), $('#you-will-receive-value'), $('#photographer-print-commission-pct'), $('#picfair-will-receive-value'), $('#picfair-commission-value')];

      for (var i = 0; i < checkForNan.length; i++) {
        if (isNaN(checkForNan[i].html())) {
          checkForNan[i].html("Error");
        }
      }

    }

    $('#user_extra_print_product_commission_pct').on('input', function () {
      recalculatePrintMargins();
    });

    $('#sample_licence_price').on('input', function () {
      recalculatePrintMargins();
    });

    $('#example_product').on('change', function () {
      recalculatePrintMargins();
    });

    $('#account_type').on('change', function () {
      recalculatePrintMargins();
    });

    $('.option-toggle').on('change', function() {

      if ($('input[type=checkbox]', this)[0].checked) {
        $('.set-print-margins-section').addClass('visible');
      } else {
        $('.set-print-margins-section').removeClass('visible');
      }

    });

    // Warn the user about unsaved data on the page
    var printsOptionsContainer = $('.prints-options-container');

    printsOptionsContainer.on('change', 'input.opt-in-images-to-prints', function (e) {
