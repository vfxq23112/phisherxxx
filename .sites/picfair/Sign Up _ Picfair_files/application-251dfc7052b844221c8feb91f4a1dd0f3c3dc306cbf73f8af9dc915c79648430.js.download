$(function() {
  var price_boxes = [
                      '.priceholderform #pic_price',
                      '.editpagegrid #user_default_price',
                      '.all_pic_price_edit_form #user_universal_pic_price'
                    ].join();

  $(price_boxes).bind('keyup mouseup', function() {
    var self = $(this);
    var price = self.val();

    var picfair_commission_pct = $('.wysiwyg_helper').data('picfair-commission-pct');
    buyer_price = buyerPrice(price, picfair_commission_pct);
    var price_string = "£" + buyer_price;
    var new_alt_currency_string = "";

    if ($('.exchange-rate').length) {
      var exchange_rate = $('.exchange-rate').data('exchange-rate');
      var currency_symbol = $('.exchange-rate').data('currency-symbol');
      var converted_price = (buyer_price * Number(exchange_rate)).toFixed(2);

      var new_alt_currency_string = " (" + currency_symbol + converted_price + ")";
    }

    $('.wysiwyg_helper strong #price-section span.current-price ').html(price_string + new_alt_currency_string);
  });


  function buyerPrice(price, picfair_commission_pct) {
    var vat_amount_percent = 0.2;
    var price = Number(price);

    var total_commission = Number(price * picfair_commission_pct);
    var total_commission_vat = Number(total_commission * vat_amount_percent);

    return (price + total_commission + total_commission_vat).toFixed(2);
  }

});
