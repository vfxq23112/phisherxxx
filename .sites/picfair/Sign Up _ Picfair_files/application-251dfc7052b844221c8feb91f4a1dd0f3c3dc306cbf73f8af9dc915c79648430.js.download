$(function() {
  // Open pic licence select overlay
  $('body').on('click', '.pic_partial__add_to_basket, .image_caption__price_box', function(e) {
    e.preventDefault();

    var pic_container = $(this).closest('.pic');

    $('.pic_licence_select, .pic_licence_select_tooltip').fadeOut(100);
    pic_container.find('.pic_licence_select').fadeIn(200);
  });

  // Close pic licence select overlay
  $('body').on('click', '.pic_licence_select .licence-exit', function(e) {
    e.preventDefault();
    var pic_container = $(this).closest('.pic');
    pic_container.find('.pic_licence_select').fadeOut(200);
    hideToolTips(true);
  });

  // Select a licence
  $('body').on('click', '.pic_licence_select .pic_licence__selector', function(e) {
    e.preventDefault();
    var pic_container = $(this).closest('.pic');
    pic_container.find('.pic_licence__selector').removeClass('active');
    pic_container.find('.button').removeClass('disabled');
    $(this).addClass('active');

    var licence;
    if ($(this).hasClass('select_advertising')) {
      licence = 'advertising';
    } else if ($(this).hasClass('select_editorial')) {
      licence = 'editorial';
    } else if ($(this).hasClass('select_standard')) {
      licence = 'standard';
    }

    pic_container.find('.pic_licence_select').data('licence', licence);
  });

  // Licence tooltips
  // Hover info icon
  $('body').on({
    mouseenter: function() {
      buildTooltipAndArrow($(this));
    },
    mouseleave: function() {
      hideToolTips();
    }
  }, '.licence-info-icon');

  // Hover fading tooltip
  $('body').on({
    mouseenter: function() {
      $('.tooltip-arrow').stop(true, false).css("opacity", "1");
      $('.licence-tooltip').stop(true, false).css("opacity", "1");
    },
    mouseleave: function() {
      hideToolTips();
    },
    click: function() {
      hideToolTips(true);
    }
  }, '.tooltips-container>.licence-tooltip');

  // Hide if user tries to scroll
  $(window).on({
    scroll: function() {
      hideToolTips(true);
    }
  });

  function buildTooltipAndArrow(element) {
    $('.licence-tooltip').stop(true, true);
    $('.tooltip-arrow').stop(true, true);

    var licenceButton = element.closest(".pic_licence__selector");

    positionArrow(licenceButton, $(".licence-tooltip-standard"));

    if (licenceButton[0].id === "select_standard") {
      setTooltipToFit($(".licence-tooltip-standard"));
      positionTooltip($(".tooltips-container>.licence-tooltip-standard"), licenceButton);
    } else if  (licenceButton[0].id === "select_editorial") {
      positionTooltip($(".tooltips-container>.licence-tooltip-editorial"), licenceButton);
    } else if  (licenceButton[0].id === "select_advertising") {
      positionTooltip($(".tooltips-container>.licence-tooltip-advertising"), licenceButton);
    }
  }

  function setTooltipToFit(tooltip) {
    if (tooltip.height() >= $(window).height()) {
      var list = $('.licence-tooltip-standard').find('.licence-tooltip-body>ul').first();
      list.hide();
    }
  }

  function positionArrow(licenceButton, tooltip) {
    arrow = $(".tooltips-container>.tooltip-arrow");

    if ( licenceButton.parent('.pic_page__licences').length > 0 ) {
      arrow.css("top", licenceButton.offset().top - arrow.outerHeight() - licenceButton.height()/2 + "px");
    }
    else if(licenceButton.parent('.pic_licence_select__content').length > 0) {
      arrow.css("top", licenceButton.offset().top + licenceButton.height()/2 - arrow.outerHeight()/2 - 80 + "px");
    }
    else if(licenceButton.parent('.licence_checkbox').length > 0) {
      arrow.css("top", licenceButton.offset().top - licenceButton.height()/2 - arrow.outerHeight() + 5 + "px");
    }else {
      arrow.css("top", (licenceButton.offset().top - licenceButton.height()) - arrow.outerHeight() + "px");
    }

    if ( isLastInRow(licenceButton, tooltip) === true ) {
      arrow.css("left", licenceButton.offset().left - arrow.outerWidth()/2 + "px");
      arrow.addClass("tooltip-arrow-right");
      arrow.css("display", "block");
    } else {
      arrow.css("left", licenceButton.offset().left + licenceButton.outerWidth() - arrow.outerWidth()/2 + "px");
      arrow.addClass("tooltip-arrow-left");
      arrow.css("display", "block");
    }
  }

  function positionTooltip(tooltip, licenceButton) {
    var tooltipArrow = $(".tooltips-container>.tooltip-arrow");
    var tooltipTitleHeight = tooltip.find(".tooltip-title-block").outerHeight();
    var bottomBuffer = 20;
    var combinedHeight = tooltip.height() + (licenceButton.offset().top - $(window).scrollTop());

    // Horizontal
    if ( isLastInRow(licenceButton, tooltip) === true ) {
      tooltip.css("left", licenceButton.offset().left - tooltip.outerWidth() - tooltipArrow.outerWidth()/2 + "px");
    } else {
      tooltip.css("left", tooltipArrow.offset().left + tooltipArrow.outerWidth() + "px");
    }

    var tooltipArrowBottomOffset = $(window).height() - (tooltipArrow.offset().top - $(window).scrollTop()) - tooltipArrow.outerHeight();

    // Vertical
    if (combinedHeight > $(window).height()) {
      if (tooltipArrowBottomOffset <= bottomBuffer) {
        tooltip.css({"top": "", "bottom": "0px"});
      }
      else if (tooltip.height() > $(window).height()) {
        tooltip.css({"top": "", "bottom": bottomBuffer + "px"});
      }
      else {
        tooltip.css({"top": "auto", "bottom": bottomBuffer + "px"});
      }

    } else {
      tooltip.css("top", (tooltipArrow.offset().top  - $(window).scrollTop()) - tooltipTitleHeight + "px");
    }

    tooltip.show();
  }

  function isLastInRow(licenceButton, tooltip) {
    var buttonWithLeft = Math.round(licenceButton.offset().left + licenceButton.outerWidth());
    return $(window).width() < buttonWithLeft + tooltip.width();
  }

  function hideToolTips(noFade) {
    var tooltips = $(".tooltips-container>.licence-tooltip");
    var tooltipArrows = $(".tooltips-container>.tooltip-arrow");

    if(noFade === true){

      tooltipArrows.stop(true, true);
      tooltips.stop(true, true);

      tooltips.attr("style", "");
      tooltips.find('.licence-included-list').attr("style", "");

      tooltipArrows.attr("style", "");
      tooltipArrows.removeClass("tooltip-arrow-left");
      tooltipArrows.removeClass("tooltip-arrow-right");

    } else {

      tooltips.fadeOut(1200, function(){
        $(this).attr("style", "");
        $(this).find('.licence-included-list').attr("style", "");
      });

      tooltipArrows.fadeOut(1200, function(){
        tooltipArrows.removeClass("tooltip-arrow-left");
        tooltipArrows.removeClass("tooltip-arrow-right");
      });
    }
  }

  // Add to basket
  $('body').on('click', '.pic_licence_select__add .pic_partial_add_to_basket', function(e) {
    e.preventDefault();
    if ($(this).hasClass('disabled')) { return false; }

    var pic_container = $(this).closest('.pic');
    pic_container.find('.select-error').remove();
    var licence_type = pic_container.find('.pic_licence_select').data('licence');

    if (licence_type == null) {
      return false;
    } else {
      var pic_id = pic_container.data('pic-id');

      $.post('/pics/add_to_basket/' + pic_id + '?licence=' + licence_type).done(function(data) {
        $('.global-nav__basket__link__text').text(data["pics_in_basket"]);
        $('body').prepend("<div class='flash'>" + data["added_pics"] + "  to basket!</div>");
        $('.flash').delay(5000).slideUp();

        $('.picfair__cart-sidebar').html(data["basket_html"]);
        $('.picfair__cart-sidebar').resize();
        pic_container.find('.pic_licence_select').fadeOut(200);
      }).fail(function(data) {
        var errorMessage = 'Sorry, there was a problem adding this image to your basket. Please try again.'
        if (data.responseJSON && data.responseJSON.error) {
          errorMessage = data.responseJSON.error;
        }
        $('.pic_licence_select__add').append("<div class='select-error' style='padding:10px;text-align:center;color:red'>"+errorMessage+"</div>");
      });
    }
  });
});
