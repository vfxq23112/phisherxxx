// pf-popup - JS open/close of popups (modals)
$(document).on('ready', function() {

  // Auto open popup on page load - needs data attribute "popup-selector"
  if ($('.pf-trigger-popup-auto-open').length > 0) {
    var popup = $($('.pf-trigger-popup-auto-open').data('popup-selector'));
    openPfPopup(popup);
  }

  // Open popup button - needs data attribute "popup-selector"
  $('body').on('click', '.pf-open-popup-button', function(e){
    e.preventDefault();
    var popup = $($(this).data('popup-selector'));
    if (popup.hasClass('is-active')) {
      closeAllPfPopups();
    }
    else {
      closeAllPfPopups();
      openPfPopup(popup, $(this));
    }
  });

  // Close popup button - closes all popups
  $('body').on('click', '.pf-popup .modal-close, .trigger-popup-close', function(e) {
    e.preventDefault();
    closeAllPfPopups();
  });

  // Escape key - anywhere in the app, closes all popups
  $(document).keyup(function(e) {
    if (e.keyCode == 27) {
      closeAllPfPopups();
    }
  });

  // Custom event: "open"
  $('.pf-popup').on('open', function(e, originalTarget) {
    openPfPopup($(e.target), originalTarget);
  });

});

function openPfPopup(popup, originalTarget) {
  popup.trigger('before-open', originalTarget);
  if (popup.hasClass('use-global-popup-background')) {
    $('.global-popup-background').addClass('is-active');
  }
  popup.addClass('is-active');
  if (popup.hasClass('pf-modal')) {
    $('html').addClass('is-clipped');
  }
}

function closeAllPfPopups() {
  $('.pf-popup').removeClass('is-active');
  $('html').removeClass('is-clipped');
  $('.global-popup-background').removeClass('is-active');
};
