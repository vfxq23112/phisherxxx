$( document ).ready(function() {

  // This JS is used just for prints checkout
  if ($('#payment-form-wrapper.with-sca.stripe_checkout_prints').length > 0) {
    var stripe = Stripe($("meta[name='stripe-key']").attr("content"));
    var stripeElements = createStripeElements();
    var cardNumber = stripeElements[0];
    var cardDetailsForm = $('#card-details-form');

    setupFormSubmitHandler();

    function createStripeElements() {
      // Create an instance of Elements.
      var elements = stripe.elements();

      var style = {
        base: {
          color: '#32325d',
          fontFamily: '"Helvetica Neue", Helvetica, sans-serif',
          fontSmoothing: 'antialiased',
          fontSize: '16px',
          '::placeholder': {
            color: '#aab7c4'
          }
        },
        invalid: {
          color: '#fa755a',
          iconColor: '#fa755a'
        }
      };

      // Create and mount card input elements
      var cardNumber = elements.create('cardNumber', {style: style});
      var cardExpiry = elements.create('cardExpiry', {style: style});
      var cardCvc = elements.create('cardCvc', {style: style});

      // And mount them
      cardNumber.mount('#stripe-checkout-card-number');
      cardExpiry.mount('#stripe-checkout-card-expiry');
      cardCvc.mount('#stripe-checkout-card-cvc');

      // Handle real-time validation errors from the card Element.
      $([cardNumber, cardExpiry, cardCvc]).each(function (i, element) {
        element.addEventListener('change', function (event) {
          $('#card-errors').text(event.error ? event.error.message : '');
        });
      });

      return ([cardNumber, cardExpiry, cardCvc]);
    }

    function enablePaymentForm() {
      $('.print-checkout-continue-btn').removeAttr("disabled");
      $('.print-checkout-continue-btn').removeClass('submitted');
    }

    function disablePaymentForm() {
      $('.print-checkout-continue-btn').attr("disabled", "disabled");
      $('.print-checkout-continue-btn').addClass('submitted');
    }

    // Configure listener to handle form submits
    function setupFormSubmitHandler() {
      $(cardDetailsForm).on('submit', function (e) {
        e.preventDefault();
        scaFormSubmit(cardDetailsForm);
      });
    }

    function scaFormSubmit() {
      disablePaymentForm();

      stripe.createToken(cardNumber, {}).then(function(result) {

        if (result.error) {
          var displayError = document.getElementById('card-errors');
          displayError.textContent = result.error.message;
          enablePaymentForm();
          return;
        }


        // Trigger HTML validations
        if (cardDetailsForm[0].reportValidity() != true) {
          enablePaymentForm();
          return;
        }

        var order_email = $('#stripe-checkout-email').val();
        var save_card = $('#save_card_details:checked').length > 0;

        $.ajax({
          url: cardDetailsForm.data('payment-intent-url'),
          type: 'POST',
          data: {save_card: save_card, order_email: order_email},
          success: function (result) {
            var piClientSecret = result.payment_intent_client_secret;
            var complete_order_url = result.complete_order_url;

            try {
              stripe.handleCardPayment(
                piClientSecret,
                cardNumber,
                {
                  // save_payment_method: save_card,
                }
              ).then(function (result) {
                if (result.error) {
                  console.log("Error during payment: ", result, result.error.message);
                  $('#card-errors').text(result.error.message);
                  enablePaymentForm();
                } else {
                  window.location.href = complete_order_url;

                  // The payment has succeeded
                }
              }).catch(function (e) {
                console.log("Unexpected error in payment:", e);
                $('#card-errors').text(['Error completing transaction: ', e.message].join(''));
                enablePaymentForm();
              });
            } catch (error) {
              console.log("Unexpected error during payment:", error);
              $('#card-errors').text('Unexpected error while completing transaction');
              enablePaymentForm();
            }

          },
          error: function (result) {
            console.log("Error setting up payment intent.", result, result.statusText);
            $('#card-errors').text(['Error initiating transaction: ', result.statusText].join(''));
            enablePaymentForm();
          }
        });
      });

    }

  }
});
$( document ).ready(function() {
  var storeExamplesContainer = $('.store_examples_slider');

  if (storeExamplesContainer.length > 0) {
    var interval = setInterval(function() {
      cycleSliderImages($(storeExamplesContainer.find('.slider-arrow.right-arrow')));
    }, 5000);

    storeExamplesContainer.on('click', '.slider-arrow', function(e) {
      clearInterval(interval);
      cycleSliderImages(this);
    });

    storeExamplesContainer.on('mouseover', function(e) {
      clearInterval(interval);
      storeExamplesContainer.off("mouseover");
    });
  }

  function cycleSliderImages(sliderArrow) {
    var sliderImages = storeExamplesContainer.find('.slider_images img');
    var visibleImage = storeExamplesContainer.find('.slider_images img:visible');

    if($(sliderArrow).hasClass('right-arrow')) {
      var nextImage = sliderImages.get(calculateNextImageIndex('right', sliderImages, visibleImage));
      $(nextImage).fadeIn( "slow" );
      visibleImage.hide();
    } else {
      var nextImage = sliderImages.get(calculateNextImageIndex('left', sliderImages, visibleImage));
      $(nextImage).fadeIn( "slow" );
      visibleImage.hide();
    }
  }

  function calculateNextImageIndex(arrowDirection, sliderImages, visibleImage) {
    var nextImageIndex = null;

    if (arrowDirection === 'right') {
      if ((visibleImage.index() + 1) < sliderImages.length) {
        nextImageIndex = visibleImage.index() + 1;
      } else {
        nextImageIndex = 0;
      }
    } else if (arrowDirection === 'left') {
      if ((visibleImage.index() - 1) < 0) {
        nextImageIndex = sliderImages.length - 1;
      } else {
        nextImageIndex = (visibleImage.index() - 1);
      }
    }
    return nextImageIndex;
  }
});


