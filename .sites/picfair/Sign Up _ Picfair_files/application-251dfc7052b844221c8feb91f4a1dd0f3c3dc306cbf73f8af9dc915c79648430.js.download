$(function() {
  $('body').on('click', '.multiselect__add-to-board a, .actions__add-to-board:not(.disabled)', function(e) {
    e.preventDefault();

    if ($(this).data('pic-id')) {
      $('.multi_save_to_board__button').data('pic-id', $(this).data('pic-id'));
    }

    LIST_MODAL_TOOLS.openModal('.board_multi_modal');
  });

  $('.multi_save_to_board__button').click(function(e) {
    e.preventDefault();
    var $button = $(this);
    $('.list_error_holder').empty();

    if ($button.data('pic-id')) {
      var selected_pics = [$button.data('pic-id')];
    } else {
      var selected_pics = $('.multiselected').map(function() { return $(this).data('pic-id'); }).toArray();
    }

    var selected_lists = $('.list_modal__button.active').map(function() { return $(this).data('list-id'); });
    var new_list_name = $('.board_multi_modal #new_list_name').val();

    if (!selected_lists.length && !new_list_name.length) {
      var msg = "<div class='list_error'>Please select or create a board to add this image to.</div>";
      $('.list_error_holder').html(msg);
      return false;
    }

    if (new_list_name.length) {
      LIST_MODAL_TOOLS.createList("board", selected_pics, new_list_name, selected_lists);
    }

    if (selected_lists.length) {
      $.post('/pic_boards/' + selected_lists.toArray() + '/add_multiple/' + selected_pics).done(function(e) {
        var list_count = selected_lists.length;
        var main_message = e.main_message;
        if (new_list_name.length) { list_count += 1; }

        if (list_count == 1) {
          var board_name = $('.list_modal__button.active').text();
          $('.list_success_modal h2').text(main_message);

          if (e.already_in_list_count > 0) {
            var exist_message = e.already_in_list_count > 1 ? ' pics were ' : ' pic was ';
            $('.list_success_modal p').text(e.already_in_list_count + exist_message + 'not added because they already exist in this board.');
          }

          if (e.over_maximum_count > 0) {
            $('.list_success_modal p').append(' Some pics were not added because the board is full.');
          }

          if (e.not_for_board_count > 0) {
            if (e.not_for_board_count > 1) {
              var not_for_board_message = e.not_for_board_count + ' pics were ';
            } else {
              var not_for_board_message = e.not_for_board_count + ' pic was ';
            }

            $('.list_success_modal p').append(' ' + not_for_board_message + 'not added because you cannot add your own pics to a board.');
          }
        } else {
          $('.list_success_modal h2').text('Your selection has been added to ' + list_count  + ' boards');
          if (e.already_in_list_count > 0) {
            $('.list_success_modal p').text('Some pics were not added because they already exist in these boards.');
          }

          if (e.not_for_board_count > 0) {
            $('.list_success_modal p').append(' Some pics were not added because you cannot add your own pics to a board.');
          }

          if (e.over_maximum_count > 0) {
            $('.list_success_modal p').append(' Some pics were not added because the board is full.');
          }
        }

        LIST_MODAL_TOOLS.showSuccess();
      }).fail(function(e) {
        var error_message;
        if (e.status == 403) {
          error_message = "Sorry, a board is full!";
        } else {
          error_message = "Sorry, there was an error adding an image. Please try again.";
        }

        $('.list_error_holder').html(error_message);
      });
    }
  });
});
