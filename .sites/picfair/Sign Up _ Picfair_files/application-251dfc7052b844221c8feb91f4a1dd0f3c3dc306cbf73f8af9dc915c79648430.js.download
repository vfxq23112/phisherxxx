var PicView = {

  subscribeToUpdates: function (path, callback) {
    if (window.App.firebaseUrl) {
      var fbRef = new Firebase(window.App.firebaseUrl + '/' + path);
      fbRef.on('value', function (snapshot) {
        callback(snapshot);
      }, function (errorObject) {
      });
    }
  },

  renderValue: function (selector, value) {
    $(selector).text(value);
  },

  suffix: function (count) {
    return (count === 1) ? 'view' : 'views';
  },

  setupOdometer: function(selector, count) {
    if ($(selector).hasClass('odometer')) { return; }

    new Odometer({
      el: $(selector)[0],
      value: count,
    });
  }

};
window.imagesLoaded = $.fn.imagesLoaded;

var HeroSearchView = Backbone.View.extend({
  defaults: {
    value: 'What are you looking for?',
    fontSize: 72,
  },

  events: {
    'keydown.hero-search__search__q': 'onKeydown',
    'keyup.hero-search__search__q': 'onKeyup',
    'input.hero-search__search__q': 'onInput',
    'mousedown.hero-search__search__q': 'onMousedown',
    'mousemove.hero-search__search__q': 'onMousemove',
    'ajax:error.hero-search__search': 'onAjaxError',
  },

  initialize: function() {
    this.$form = this.$el.find('.hero-search__search');
    this.$field = this.$el.find('.hero-search__search__field');
    this.$input = this.$el.find('.hero-search__search__q');
    this.$placeholder = this.$el.find('.hero-search__search__placeholder');
    this.$caret = $('<div class="hero-search__search__caret" />').appendTo(this.$field);

    this.$form.on('ajax:beforeSend', function(event, xhr, settings) {
      settings.timeout = 10000;
    });

    var hasSearch = !!this.$input.length;

    if (hasSearch) {
      this.focusAtEnd();
      this.render();
    }

    this.fallbacks();

    if (hasSearch) {
      this.$input.focus(_.bind(this.onFocus, this));
      this.$input.blur(_.bind(this.onBlur, this));

      $(window).resize(_.throttle(_.bind(this.render, this), 100));

      this.debouncedSearch = _.debounce(_.bind(this.search, this), 250);
    }

  },

  fallbacks: function() {
    this.objectFitFallback();

    if (this.isIe()) {
      this.$caret.remove();
    }
  },

  objectFitFallback: function() {
    if (Modernizr.objectfit) { return; }

    var $img = this.$el.find('.hero-search__hero__image img').last();

    $img.parent().imagesLoaded(function() {
      $img.css({ width: 'auto', height: 'auto' });

      $img.fill({
        width: $img.width(),
        height: $img.height(),
      });
    });
  },

  render: function() {
    this.$el.toggleClass('is-mobile', this.isMobile());

    this.setFontSize();
    this.updateCaretPosition();
  },

  onFocus: function() {
    this.updateCaretPosition();
  },

  onBlur: function() {
    this.hideCaret();

    if (this.isEmpty()) {
      this.reset();
    }
  },

  onKeydown: function() {
    this.updateCaretPosition();
  },

  onKeyup: function() {
    this.updateCaretPosition();
  },

  onInput: function() {
    if (this.isDefault()) {
      this.clear();
    }

    this.setPlaceholder();
    this.setFontSize();
  },

  onMousedown: function() {
    this.alwaysMouseUp();

    if (this.isDefault()) {
      return this.clear();
    }

    this.updateCaretPosition();
  },

  onMousemove: function() {
    if (this.mouseIsDown) {
      this.updateCaretPosition();
    }
  },

  search: function() {
    $.get('/?format=js&q=' + this.getValue());
  },

  alwaysMouseUp: function() {
    this.mouseIsDown = true;

    $(window).one('mouseup', _.bind(function() {
      this.mouseIsDown = false;
    }, this));
  },

  onAjaxError: function() {
    var $error = $('<span class="hero-search__search__sorry">Sorry, something went wrong. Please try again.</span>');

    this.$el.find('.hero-search__loading').hide();
    this.$el.find('.hero-search__search__buttons').show().html($error);
  },

  setValue: function(value) {
    this.$input.val(value);
    this.setPlaceholder();
  },

  getValue: function() {
    return this.$input.val();
  },

  setPlaceholder: function() {
    var value = this.getValue();
    var inner = $('<div />');

    inner.append('<span />');

    for (var i = 0; i < value.length; i++) {
      inner.append('<span>' + value[i] + '</span>');
    }

    this.$placeholder.html(inner);
  },

  clear: function() {
    var currentValue = this.getValue();
    var defaultValue = this.defaults.value;

    this.$form.addClass('did-clear');
    this.setValue(currentValue.replace(defaultValue, ''));
    this.updateCaretPosition();
  },

  reset: function() {
    this.setValue(this.defaults.value);
  },

  setFontSize: function() {
    var fontSize = this.getFontSize();
    var width = $(window).width() * 0.8;

    for (var i = 0; i < 100; i++) {
      if (this.$field.width() >= width) {
        this.$field.css('font-size', --fontSize);
      } else {
        break;
      }
    }

    for (var i = 0; i < 100; i++) {
      if (this.$field.width() < width && fontSize < this.defaults.fontSize) {
        this.$field.css('font-size', fontSize++);
      } else {
        break;
      }
    }

    this.$field.css('opacity', 1);
  },

  getFontSize: function() {
    return parseInt(this.$field.css('font-size'));
  },

  isMobile: function() {
    return $(window).width() <= 640;
  },

  isDefault: function() {
    return this.getValue().indexOf(this.defaults.value) !== -1;
  },

  isEmpty: function() {
    return this.getValue() === '';
  },

  focusAtEnd: function() {
    this.setValue(this.getValue());

    if (this.$input[0].setSelectionRange) {
      var length = this.getValue().length;
      this.$input[0].setSelectionRange(length, length);
    }
  },

  isSelectingRange: function() {
    return this.$input[0].selectionStart !== this.$input[0].selectionEnd;
  },

  isFocused: function() {
    return this.$input.is(document.activeElement);
  },

  isBlurred: function() {
    return !this.isFocused();
  },

  updateCaretPosition: function() {
    this.stopCaretBlinking();

    var updateCaretPosition = _.bind(function() {
      if (this.isSelectingRange() || this.isBlurred()) {
        this.hideCaret();
        return;
      }

      var index = this.$input[0].selectionEnd;
      var character = this.$placeholder.find('span').eq(index);
      var right = character.position().left + character.width();

      if (this.getValue().length) {
        this.$caret.css('right', this.$placeholder.innerWidth() - right);
      } else {
        this.$caret.css('right', '50%');
      }

      this.showCaret();
      this.startCaretBlinking();
    }, this);

    return _.defer(updateCaretPosition);
  },

  showCaret: function() {
    this.$caret.show();
  },

  hideCaret: function() {
    this.$caret.hide();
  },

  stopCaretBlinking: function() {
    this.$caret.removeClass('is-blinking');
  },

  startCaretBlinking: function() {
    var startCaretBlinking = _.bind(function() {
      this.$caret.addClass('is-blinking');
    }, this);

    _.debounce(startCaretBlinking, 500).call();
  },

  isIe: function() {
    return window.navigator.userAgent.search(/MSIE|Trident|Edge/) !== -1;
  },
});

