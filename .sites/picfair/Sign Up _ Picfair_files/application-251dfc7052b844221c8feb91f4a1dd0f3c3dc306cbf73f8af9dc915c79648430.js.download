  function ensureAgainstOrphans() {
    if ($('.category__layout__pic.hidden').length > 0) {
      $('.category__layout__pic.hidden').hide();
      var $lastVisiblePic = $('.category__layout__pic:visible').last();

      while (notInLastColumn($lastVisiblePic) && $('.category__layout__pic:visible').length > 22) {
        $('.category__layout__pic:hidden').first().css('display', 'inline-block');
        $lastVisiblePic = $('.category__layout__pic:visible').last();
      }
    }
  }

  function notInLastColumn(pic) {
    var pic_r_offset = $(window).width() - (pic.offset().left + pic.outerWidth());
    return pic_r_offset > 100;
  }

  // Click on icon inside input box to submit
  $('body').on('click', '.secondary-search-icon', function (event) {
