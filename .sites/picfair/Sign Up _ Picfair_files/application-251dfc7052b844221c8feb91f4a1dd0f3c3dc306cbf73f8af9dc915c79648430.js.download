function switchBadge(button) {
  var container = button.closest('.post_preview_container');
  var new_image = button.data('image-url');
  var image_to_change = container.find('img');
  var download_button = container.find('.resources-download a');
  var download_url = new_image.match(/resources\/badges\/.+$/)[0];
  var new_download_link = download_button.attr('href').replace(/resources\/badges.+$/, download_url);

  if (new_image.match(/transparent/)) {
    container.find('.badge-image').addClass('transparent');
  } else {
    container.find('.badge-image').removeClass('transparent');
  }

  image_to_change.attr('src', new_image);
  download_button.attr('href', new_download_link);
};
