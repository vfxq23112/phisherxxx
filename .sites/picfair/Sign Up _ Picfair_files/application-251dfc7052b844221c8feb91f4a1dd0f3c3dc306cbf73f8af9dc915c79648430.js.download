function handleDpiDisplay() {
  if (!isSelectedProductPlaceholder()) {
    var selectedProduct = $('select#size').find(':selected');

    // Selecting a size with metadata
    var dpiInfo = calculateDpi(selectedProduct);
    userDpi = $('#user-dpi');

    userDpi.find('span.current-user-dpi').text(dpiInfo.dpi);
    userDpi.show();

    var dpiContainer = $('#dpi');
    if (dpiContainer.length > 0) {
      // Recommended
      dpiContainer.find('span.min-size-px').text(dpiInfo.recommendedSizeText);
      dpiContainer.find('span.current-dpi-info').text(dpiInfo.dpi);
      // Current
      dpiContainer.find('span.current-size-px').text(dpiInfo.currentSizeText);
      dpiContainer.find('span.dpi-info').text($(selectedProduct).data('recommended-dpi')); // here
    }

    $('.recommended-user-dpi-warning').hide();
    var recommendedDpi = parseInt($(selectedProduct).data('recommended-dpi')); // here
    if (dpiInfo.dpi < recommendedDpi) {
      $('.recommended-user-dpi-warning').show();
      $('.recommended-user-dpi-warning span').text(recommendedDpi);
    }
  }
}

function isSelectedProductPlaceholder() {
  var selectedProduct = $('select#size').find(':selected');
  return !selectedProduct.data('size');
}

function calculateDpi(selectedProduct) {
  var sizesInInches = $(selectedProduct).data('printable-size-inches').split('x').map(Number);
  var sizesInPixels = [$(selectedProduct).data('min-width'), $(selectedProduct).data('min-height')];

  // Raw originals / Crop originals
  var widthDifference = $('.pic-preview').data('raw-image-width') / JSON.parse($('#crop-data').val()).originalWidth;
  var heightDifference = $('.pic-preview').data('raw-image-height') / JSON.parse($('#crop-data').val()).originalHeight;

  if ($(selectedProduct).data('orientation') === 'Landscape') {

    var fullCropWidth = Math.round(JSON.parse($('#crop-data').val()).width * widthDifference);
    var fullCropHeight = Math.round(JSON.parse($('#crop-data').val()).height * widthDifference);

    // Recommended size: width & height px
    var recommendedSizeText = Math.max.apply(null, sizesInPixels) + 'x' + Math.min.apply(null, sizesInPixels);
    // Current size
    var currentSizeText = fullCropWidth + 'x' + fullCropHeight;
    // DPI: crop width / width inches
    var dpi = Math.round(fullCropWidth / Math.max.apply(null, sizesInInches));

  } else {
    var fullCropWidth = Math.round(JSON.parse($('#crop-data').val()).height * heightDifference);
    var fullCropHeight = Math.round(JSON.parse($('#crop-data').val()).width * heightDifference);

    // Recommended size: height & width px
    var recommendedSizeText = Math.min.apply(null, sizesInPixels) + 'x' + Math.max.apply(null, sizesInPixels);
    // Current Size
    var currentSizeText = fullCropHeight + 'x' + fullCropWidth;
    // DPI: crop height / height inches
    var dpi = Math.round(fullCropHeight / Math.min.apply(null, sizesInInches));
  }

  return dpiInfo = { dpi: dpi, recommendedSizeText: recommendedSizeText, currentSizeText: currentSizeText };
}

// This function currently doesn't work on page load because the calculateDpi function requires a preview image size
// we only have one preview image on page load, the first option will be correct but all the others won't
function disableSelectOptionsUnderDpiThreshold(sizeListOptions) {
  for (i = 0; sizeListOptions.length > i; i++) {
    var currentlySelected = sizeListOptions[i];
    var dpi = calculateDpi(currentlySelected).dpi;
    if (dpi < DPI_THRESHOLD) {
      $(currentlySelected).attr('disabled', 'disabled');
    }
  }
}

function isDpiBelowThreshold() {
  var selectedProduct = $('select#size').find(':selected');
  dpiInfo = calculateDpi(selectedProduct);
  if (dpiInfo.dpi < DPI_THRESHOLD) {
    return true;
  } else {
    return false;
  }
}

