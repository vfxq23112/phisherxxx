function adjustProductPreview(currentSize) {
  if (isSelectedProductPlaceholder()) {
    // a placeholder size is selected, show placeholder
    $('.pic-preview-container').hide();
    $('.pic-preview-placeholder').addClass('pic-preview-placeholder-show');

    // user dpi section is shown again once handleDpiDisplay() is called
    $('#user-dpi').hide();
  } else {
    // a size is selected, show preview
    $('.pic-preview-container').show();
    $('.pic-preview-placeholder').removeClass('pic-preview-placeholder-show');

    $('.pic-preview').css({ "width": "", "height": "" });
    var containerWidth = $('.pic-preview-container').width();
    var width = $(currentSize).data('min-width');
    var height = $(currentSize).data('min-height');

    calculateWidthHeightToPixels(containerWidth, width, height);
    if ($('#size').val() !== "") {
      $('#print-page-main-content').hide();
      $('#print-crop-section').show();

      initializeCropper();
    }

    $(window).scrollTop(0);
  }
}

function calculateWidthHeightToPixels(containerWidth, width, height) {
  var originalWidth = $(".pic-preview").data("rawImageWidth");
  var originalHeight = $(".pic-preview").data("rawImageHeight");
  var printAspectRatio = width / height;
  var cropData;
  if (height <= width) {
    // landscape
    var smallestSize = Math.floor((containerWidth / width) * height);
    $('.pic-preview').height(smallestSize + "px");
  } else {
    // portrait
    $('.pic-preview').height($('.pic-preview-container').width());
    var smallestSize = Math.floor((containerWidth / height) * width);
    $('.pic-preview').width(smallestSize + "px");
  }
  if (printAspectRatio < originalWidth / originalHeight) {
    // Width will be cropped
    var cropWidth = Math.floor(originalHeight * printAspectRatio);
    var x = (originalWidth - cropWidth) / 2;
    cropData = { "x": x, "y": 0, "width": cropWidth, "height": originalHeight, "scaleX": 1, "scaleY": 1, "originalWidth": originalWidth, "originalHeight": originalHeight };
  } else {
    // Height will be cropped
    var cropHeight = Math.floor(originalWidth / printAspectRatio);
    var y = (originalHeight - cropHeight) / 2;
    cropData = { "x": 0, "y": y, "width": originalWidth, "height": cropHeight, "scaleX": 1, "scaleY": 1, "originalWidth": originalWidth, "originalHeight": originalHeight };
  }

  $('#crop-data').val(JSON.stringify(cropData));
  handleDpiDisplay();
}

