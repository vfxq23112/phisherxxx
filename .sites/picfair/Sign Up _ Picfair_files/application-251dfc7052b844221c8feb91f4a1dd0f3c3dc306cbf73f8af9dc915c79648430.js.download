      $('.change-licence-types-modal .button.next').addClass('disabled');
    else
      $('.change-licence-types-modal .button.next').removeClass('disabled');

    $(this).closest('ul').find('li.licence-types-option.selected').removeClass('selected');
    $(this).closest('li').addClass('selected');
  });

  $('body').on('click', '.selectable.licence-types-option', function() {
