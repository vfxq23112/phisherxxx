(function() {
  var CONFIRMATION_TEXT = "Images that may contain sensitive content will no longer be obscured. Are you sure?";
  var safe_search_modal = new ConfirmationModal('safe-search', CONFIRMATION_TEXT, "OK");

  $(document).on('submit', '.disable-safe-search', function(e) {
    e.preventDefault();

    if (isMobile()) {
      mobileConfirmation();
    } else {
      desktopConfirmation();
    }
  });

  $(document).on('click', '.safe-search-confirmation [data-action="cancel"]', cancel);
  $(document).on('click', '.safe-search-confirmation [data-action="ok"]', disableSafeSearch);

  function isMobile() {
   return $(window).width() <= 640;
  }

  function desktopConfirmation() {
    $('body').append(safe_search_modal.modalHtml());
  }

  function mobileConfirmation() {
    if (window.confirm(CONFIRMATION_TEXT)) {
      disableSafeSearch();
    }
  }

  function disableSafeSearch() {
    $(document).off('submit', '.disable-safe-search');
    $('.disable-safe-search').submit();
  }

  function cancel() {
    $('.safe-search-confirmation').remove();
  }
})();
