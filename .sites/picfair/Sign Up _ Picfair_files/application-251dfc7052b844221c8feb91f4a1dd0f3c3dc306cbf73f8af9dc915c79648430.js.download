    $('textarea').val(embed_code);

    $('.resources-embed__edit input, .resources-embed__edit select').change(function() {
      var theme_value = $('.resources-embed__edit input#dark').is(':checked') ? "dark" : "light";
      var name_value = $('.resources-embed__edit input#name').is(':checked') ? "false" : "true";
      var grid_value = $('.resources-embed__edit select#grid').val().split('x');
      var album_id = $('.resources-embed__edit select#album').val();
      var x_value = grid_value[0];
      var y_value = grid_value[1];
      var height_value = (parseInt(y_value) * 150) + 100;
      var code = embed_code;

      code = code.replace(/theme='\w+'/, "theme='" + theme_value + "'");
      code = code.replace(/name='\w+'/, "name='" + name_value + "'");
      code = code.replace(/x='\w+'/, "x='" + x_value + "'");
      code = code.replace(/y='\w+'/, "y='" + y_value + "'");
      code = code.replace(/album='((\w+['-]?)*)'/, "album='" + album_id + "'");

      $('textarea').val(code);
